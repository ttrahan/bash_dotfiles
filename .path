#! /bin/bash -e

# export paths

# Local binaries
export PATH="$PATH:$HOME/bin"

# AWS CLI
export PATH=$PATH:/usr/local/bin/aws

# Google Cloud SDK.
if [[ -f /Users/tomtra/google-cloud-sdk/path.bash.inc ]]; then
  source '/Users/tomtra/google-cloud-sdk/path.bash.inc'
fi

# Python 3.4
# The original version is saved in .bash_profile.pysave
export PATH="/Library/Frameworks/Python.framework/Versions/3.4/bin:${PATH}"

# DC/OS CLI
export PATH=$PATH:/Users/tomtra/dcos/bin;

# Terraform
export PATH=$PATH:$HOME/third-party-tools/terraform

# PhantomJS
export PATH="$PATH:/usr/lib/phantomjs-2.0.0-macosx/bin"

# MongoDB
export PATH=$PATH:/usr/local/Cellar/mongodb/3.0.7

# Google App Engine
export PATH=$PATH:/usr/local/bin/go_appengine

# MySQL
export PATH=$PATH:/usr/local/mysql/bin
