#! /bin/bash -e

declare -A pairs
pairs=( \
  [Google Cloud SDK]=$HOME'/google-cloud-sdk/completion.bash.inc' \
  [test2]=test2 \
)

for i in "${!pairs[@]}"; do
  j=${pairs[$i]}
  echo "$i.txt" "$j.txt"
done


# Enable bash completion for gcloud.
if [[ -f $HOME'/google-cloud-sdk/completion.bash.inc' ]]; then
  source $HOME'/google-cloud-sdk/completion.bash.inc'
else
  echo 'Google bash completion not found...not loaded'
fi

# Enable bash completion for docker machine
if [[ -f $HOME'/.docker-machine-completion.sh' ]]; then
  source $HOME'/.docker-machine-completion.sh'
else
  echo 'Docker Machine bash completion not found...not loaded'
fi

# enable bash completion for git
if [[ -f $HOME'/.git-completion.bash' ]]; then
  source $HOME'/.git-completion.bash'
else
  echo 'Git bash completion not found...not loaded'
fi

# enable bash completion for AWS CLI
AWSCLI_DIR=/usr/local/bin/aws_completer
complete -C $AWSCLI_DIR aws
